{
  "name": "problem-solving-env",
  "version": "0.0.1",
  "description": "Environment for problem solving with TS in a TDD way",
  "private": true,
  "scripts": {
    "fix:format": "npm run test:format -- --write",
    "fix:lint": "npm run test:lint -- --fix",
    "test:run": "vitest run",
    "test": "vitest",
    "test:format": "prettier --check \"src/**/*.ts\"",
    "test:lint": "eslint --ext=ts src",
    "test:types": "tsc -p tsconfig.json --noEmit --skipLibCheck",
    "lint:staged": "lint-staged"
  },
  "author": "Patryk Lizo≈Ñ",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.23.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.15.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "vitest": "^0.24.1"
  }
}
